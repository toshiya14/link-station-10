<Window x:Class="RMEGo.Sunflower.LinkStation10.Launch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RMEGo.Sunflower.LinkStation10"
        xmlns:controls="clr-namespace:RMEGo.Sunflower.LinkStation10.Controls"
        mc:Ignorable="d"
        Title="Launch" Visibility="Visible" Height="1" Width="1" Background="{x:Null}" AllowsTransparency="True" WindowStyle="None">
    <Grid>
        <Popup Placement="AbsolutePoint" HorizontalAlignment="Left" VerticalAlignment="Top" IsOpen="True" Margin="0" VerticalOffset="100" AllowsTransparency="True">
            <Grid Margin="0,0,5,5">
                <controls:TileWrapper x:Name="TileBar">
                    <controls:TileWrapper.Effect>
                        <DropShadowEffect Opacity="0.5" ShadowDepth="3" BlurRadius="5"/>
                    </controls:TileWrapper.Effect>
                </controls:TileWrapper>
            </Grid>
        </Popup>
    </Grid>
</Window>
